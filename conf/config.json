{

    "HeartBeat":{
       "LoggerHeartBeat": {
            "module": "stream.build_in_heartbeats.logger_heart_beat.LoggerHeartBeat"
        }
    },

    "TriggerTemplate": {
        "RabbitMQTrigger": {
            "module": "stream.build_in_triggers.rmq_trigger.RabbitMQTrigger",
            "args":{
                "url": "amqp://user:pass@host:port/%2F.queue",
                "need_ack": false,
                "mode": "normal"
            }
        },
        "TimerTrigger": {
            "module": "stream.build_in_triggers.test_trigger.TimerTrigger",
            "pool_size" : 2,
            "args": {
                "poll_time": 1
            }
        }
    },

    "NodeTemplate": {

         "PrintNode1" : {
            "module": "stream.build_in_nodes.test_node.PrintNode",
        },
        "PrintNode2" : {
             "module": "stream.build_in_nodes.test_node.PrintNode",
             "pool_size": 3,
             "mode": "thread"
        },
         "AddTailNode1" : {
            "module": "stream.build_in_nodes.test_node.AddTailNode",
            "pool_size": 1
        },
        "DelayNode" : {
            "module": "stream.build_in_nodes.test_node.DelayNode",
            "pool_size": 1,
            "mode": "thread",
            "args":{
                "loop": 5
            }

        },
        "DelayNode2" : {
            "module": "stream.build_in_nodes.test_node.DelayNode",
            "pool_size": 1,
            "mode": "thread",
            "args":{
                "loop": 50
            }
        }


    },

    "Streams": {
//        "TestStream": {
//            "trigger": ["TimerTrigger"],
//            "process": ["AddTailNode1", "PrintNode1", "AddTailNode2", "PrintNode2"]
//        },
        "RmqTestStream": {
            "trigger": ["RabbitMQTrigger"],
            "process": ["DelayNode", "DelayNode2", "PrintNode1"]
        }
    }
}
