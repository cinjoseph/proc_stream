{
    "TriggerTemplate": {
        "TimerTrigger": {
            "module": "stream.build_in_triggers.test_trigger.TimerTrigger",
            "args": {
                "sleep_time": 1
            }
        }
    },

    "NodeTemplate": {
         "PrintNode1" : {
             "module": "stream.build_in_nodes.test_node.PrintNode",
        },
        "PrintNode2" : {
             "module": "stream.build_in_nodes.test_node.PrintNode",
             "pool_size": 3,
             "mode": "thread"
        },
         "AddTailNode1" : {
            "module": "stream.build_in_nodes.test_node.AddTailNode",
            "pool_size": 1
        },
        "AddTailNode2" : {
            "module": "stream.build_in_nodes.test_node.AddTailNode",
            "pool_size": 1
        }


    },

    "Streams": {
        "TestStream": {
            "trigger": ["TimerTrigger"],
            "process": ["AddTailNode1", "PrintNode1", "AddTailNode2", "PrintNode2"]
        }
    }
}
